pipeline {

    agent any

    //environment {
        //DACO = credentials("secVal")
    //}

    stages {
        stage('Building app') {
                steps {
                    sh "npm install"
                    sh "npm run build"
                }
        }
        stage('Copying') {
                steps {
                    sh "tar cvf build.tar build"
                }
        }
    }
}
