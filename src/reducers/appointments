import { combineReducers } from 'redux';
import { FETCH_APPOINTMENTS_RESPONSE } from '../helper/dispatchTypes';

const appointmentsSearchResults = (state = [], action) => {
  switch (action.type) {
    case FETCH_APPOINTMENTS_RESPONSE:
      return action.appointments;
    default:
      return state;
  }
};

const appointmentSearch = combineReducers({ appointmentsSearchResults });

export default appointmentSearch;
