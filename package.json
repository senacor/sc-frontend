    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.22",
    "@material/slider": "^0.36.0",
    "ajv": "^6.0.0",
    "jest-html-reporter": "^2.4.2",
    "jest-matcher-utils": "^23.6.0",
    "lodash": "^4.17.10",
    "material-ui-slider": "^0.1.0",
    "moment": "^2.20.1",
    "moment-range": "^4.0.1",
    "moment-timezone": "^0.5.21",
    "object-get": "^2.1.0",
    "object-set": "^1.0.1",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "react-swipe": "^5.1.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:report": "react-scripts test --env=jsdom --reporters=default --reporters=jest-html-reporter",
    "eject": "react-scripts eject",
    "eslint": "./node_modules/.bin/eslint --max-warnings 0 src",
    "eslintFix": "./node_modules/.bin/eslint --fix src"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
