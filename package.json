{
  "name": "pr-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.5.1",
    "@material-ui/icons": "3.0.1",
    "@material-ui/lab": "3.0.0-alpha.23",
    "@material/slider": "0.41.0",
    "ajv": "^6.0.0",
    "lodash": "^4.17.10",
    "material-ui-slider": "3.0.0",
    "moment": "^2.20.1",
    "moment-range": "^4.0.1",
    "moment-timezone": "^0.5.21",
    "object-get": "^2.1.0",
    "object-set": "^1.0.1",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.1",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.5",
    "react-swipe": "5.1.1",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:report": "react-scripts test --env=jsdom --reporters=default --reporters=jest-html-reporter",
    "eject": "react-scripts eject",
    "eslint": "./node_modules/.bin/eslint --max-warnings 0 src",
    "eslintFix": "./node_modules/.bin/eslint --fix src"
  },
  "devDependencies": {
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.4",
    "eslint-plugin-prettier": "^2.6.2",
    "fetch-mock": "6.5.2",
    "jest-html-reporter": "2.4.2",
    "jest-localstorage-mock": "2.3.0",
    "jest-matcher-utils": "23.6.0",
    "prettier": "1.15.2",
    "redux-mock-store": "1.5.3",
    "timezone-mock": "1.0.2"
  },
  "proxy": {
    "/oauth2": {
      "target": "http://localhost:8000"
    },
    "/api/v1/tasks": {
      "target": "http://localhost:8000"
    },
    "/api/v1/prs": {
      "target": "http://localhost:8010"
    },
    "/api/v1/employees": {
      "target": "http://localhost:8010"
    },
    "/api/v1/cstmembers": {
      "target": "http://localhost:8010"
    },
    "/api/v1/events": {
      "target": "http://localhost:8010"
    },
    "/api/v1/hr": {
      "target": "http://localhost:8010"
    },
    "/api/v1/user": {
      "target": "http://localhost:8010"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "jest-html-reporter": {
    "pageTitle": "PR-App Unit Tests",
    "outputPath": "target/Test-Reports/Unit-Tests.html",
    "includeFailureMsg": true,
    "dateFormat": "dd-mm-yyyy HH:MM:ss",
    "sort": "titleAsc"
  }
}
