    "timezone-mock": "^1.0.0"
  },
  "proxy": {
    "/oauth2": {
      "target": "http://localhost:8000"
    },
    "/api/v1/tasks": {
      "target": "http://localhost:8000"
    },
    "/api/v1/prs": {
      "target": "http://localhost:8010"
    },
    "/api/v1/employees": {
      "target": "http://localhost:8010"
    },
    "/api/v1/cstmembers": {
      "target": "http://localhost:8010"
    },
    "/api/v1/events": {
      "target": "http://localhost:8010"
    },
    "/api/v1/hr": {
      "target": "http://localhost:8010"
    },
    "/api/v1/user": {
      "target": "http://localhost:8010"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "jest-html-reporter": {
    "pageTitle": "PR-App Unit Tests",
    "outputPath": "target/Test-Reports/Unit-Tests.html",
    "includeFailureMsg": true,
    "dateFormat": "dd-mm-yyyy HH:MM:ss",
    "sort": "titleAsc"
  }
